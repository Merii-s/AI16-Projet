<body>

    <%- include('nav.ejs') %>

    <h1>
        <%= title %>
    </h1>
    <table border>
        <thead>
            <tr>
                <th>Firstname</th>
                <th>Lastname</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% recruiters.forEach((recruiter)=> { %>
                <tr>
                    <td>
                        <%= recruiter.prenom %>
                    </td>
                    <td>
                        <%= recruiter.nom %>
                    </td>
                    <td>
                        <%= recruiter.email %>
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>
</body>