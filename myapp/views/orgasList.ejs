<body>

    <%- include('nav.ejs') %>

    <h1>
        <%= title %>
    </h1>
    <table border>
        <thead>
            <tr>
                <th>SIREN</th>
                <th>nom</th>
                <th>type</th>
                <th>Siege Social</th>
                <th>Accepter</th>
                <th>Refuser</th>
            </tr>
        </thead>
        <tbody>
         <form id="AdminOrga" action="/organisations/setadminorga" method="POST">
            <% orgas.forEach((orga)=> { %>
                <tr>
                    <td>
                        <%= orga.SIREN %>
                    </td>
                    <td>
                        <%= orga.nom %>
                    </td>
                    <td>
                        <%= orga.type %>
                    </td>
                    <td>
                        <%= orga.siegeSocial %>
                    </td>
                    <td>
                        <input type="submit" name="accepter" value="<%= orga.SIREN %>,<%= orga.emailusers%>"> 
                    </td>
                    <td>
                        <input type="submit" name="rejeter" value="<%= orga.SIREN%>,<%= orga.emailusers%>"> 
                    </td>

                </tr>
                <% }) %>

         </form>
        </tbody>
    </table>
</body>