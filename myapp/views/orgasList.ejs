<body>
    <h1>
        <%= title %>
    </h1>
    <table border>
        <thead>
            <tr>
                <th>SIREN</th>
                <th>nom</th>
                <th>type</th>
                <th>Siege Social</th>
                <th>approuve</th>
            </tr>
        </thead>
        <tbody>
            <% orgas.forEach((orga)=> { %>
                <tr>
                    <td>
                        <%= orga.SIREN %>
                    </td>
                    <td>
                        <%= orga.nom %>
                    </td>
                    <td>
                        <%= orga.type %>
                    </td>
                    <td>
                        <%= orga.siegeSocial %>
                    </td>
                    <td>
                        <%= orga.approuve %>
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>
</body>