<body>
    <h1>
        <%= title %>
    </h1>
    <table border>
        <thead>
            <tr>
                <th>Id Fiche</th>
                <th>Intitulé</th>
                <th>StatutPoste</th>
                <th>Responsable Hierarchique</th>
                <th>Type Metier</th>
                <th>Lieu</th>
                <th>Rythme</th>
                <th>Salaire</th>
                <th>Description</th>
            </tr>
        </thead>
        <tbody>
            <% fiches.forEach((fiche)=> { %>
                <tr>
                    <td>
                        <%= fiche.id %>
                    </td>
                    <td>
                        <%= fiche.intitulé %>
                    </td>
                    <td>
                        <%= fiche.statutPoste %>
                    </td>
                    <td>
                        <%= fiche.RespHierarchique %>
                    </td>
                    <td>
                        <%= fiche.typeMetier %>
                    </td>
                    <td>
                        <%= fiche.lieu %>
                    </td>
                    <td>
                        <%= fiche.rythme %>
                    </td>
                    <td>
                        <%= fiche.salaire %>
                    </td>
                    <td>
                        <%= fiche.description %>
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>
</body>